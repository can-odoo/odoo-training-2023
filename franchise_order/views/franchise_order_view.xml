<odoo>

    <record id="franchise_product_order_list" model="ir.ui.view">
        <field name="name">Order List</field>
        <field name="model">franchise.product.stock</field>
        <field name="inherit_id" ref="franchise_ERP.franchise_product_stock_tree"/> 
        <field name="arch" type="xml">
            <xpath expr="//tree/field[@name='status']" position="after">
                <field name="payment"/>
                <field name="state"/>
            </xpath>

            <xpath expr="//tree/field[@name='status']" position="attributes">
                <attribute name="invisible">1</attribute>
            </xpath>

        </field>
    </record> 
    
    <!-- Action -->
    <record id="franchise_order_action" model="ir.actions.act_window">
        <field name="name">Franchise order</field>
        <field name="res_model">franchise.product.stock</field>
        <field name="view_mode">tree,form</field>
    </record>

</odoo>