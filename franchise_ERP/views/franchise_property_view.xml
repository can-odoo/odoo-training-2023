<?xml version="1.0" encoding="UTF-8"?>

<odoo>

    <!-- List -->
    <record id="franchise_property_form" model="ir.ui.view">
        <field name="name">Franchise.property</field>
        <field name="model">franchise.property</field>
        <field name="arch" type="xml">  
            <tree string="Channel" >
                <field name="name"/>
                <field name="franchise_owner"/>    
            </tree>
        </field>
    </record>

    <!-- Action -->
    <record id="franchise_property_action" model="ir.actions.act_window">
        <field name="name">Franchise Property Action</field>
        <field name="res_model">franchise.property</field>
        <field name="view_mode">tree,form</field>
    </record>

      <!-- Menu -->
      <menuitem id="franchise_menu_root" name="Franchise">
        <menuitem id="franchise_first_level_menu" name="Explore">
            <menuitem id="franchise_model_menu_action" action="franchise_property_action" name="Franchies"/>
        </menuitem>
    </menuitem>
</odoo>