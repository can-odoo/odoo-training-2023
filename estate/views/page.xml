<odoo>
<record id="property_view" model="website.page">
    <field name="name">Properties Page</field>
    <field name="website_published">True</field>
    <field name="url">/properties</field>
    <field name="type">qweb</field>
    <field name="key">theme_tutorial.services_page</field>
</record>

<record id="property_page_link" model="website.menu">
    <field name="name">Properties</field>
    <field name="page_id" ref="property_view"/>
